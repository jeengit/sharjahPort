{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.demo.sharjahPort",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZSPA_DEMO_SRV": {
				"uri": "/sap/opu/odata/sap/ZSPA_DEMO_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/ZSPA_DEMO_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.demo.sharjahPort.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.demo.sharjahPort.i18n.i18n"
				}
			},
			"s4Model": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZSPA_DEMO_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.demo.sharjahPort.view.customer",
				"controlAggregation": "detailPages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "loginC",
					"pattern": "login",
					"target": [
						"loginC"
					]
				},
				{
					"name": "registerC",
					"pattern": "CustomerReg",
					"target": [
						"registerC"
					]
				},
				{
					"name": "loginOTP",
					"pattern": "login",
					"target": [
						"loginOTP"
					]
				},
				{
					"name": "dashboard",
					"pattern": "dashboard",
					"target": [
						"home",
						"dashboard"
					]
				},
				{
					"name": "products",
					"pattern": "items={sPath}&type='ETA'",
					"target": [
						"home",
						"products"
					]
				},
				{
					"name": "productDetails",
					"pattern": "productDetails/{sPath}/{id}",
					"target": [
						"home",
						"productDetails"
					]
				},
				{
					"name": "orderNow",
					"pattern": "orderNow",
					"target": [
						"home",
						"orderNow"
					]
				},
				{
					"name": "orders",
					"pattern": "items={sPath}&type='LOG'",
					"target": [
						"home",
						"orders"
					]
				},
				{
					"name": "orderStatus",
					"pattern": "orderStatus/{sPath}/{id}",
					"target": [
						"home",
						"orderStatus"
					]
				},
				{
					"name": "vessel",
					"pattern": "vessel",
					"target": [
						"home",
						"vessel"
					]
				},
				{
					"name": "dashboardHarbour",
					"pattern": "dashboardHarbour",
					"target": [
						"home",
						"dashboardHarbour"
					]
				},
				{
					"name": "dashboardAgent",
					"pattern": "dashboardAgent",
					"target": [
						"home",
						"dashboardAgent"
					]
				}
			],
			"targets": {
				"home": {
					"viewType": "XML",
					"transition": "slide",
					"controlAggregation": "masterPages",
					"viewName": "Home"
				},
				"loginC": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerLogin"
				},
				"registerC": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerRegistration"
				},
				"loginOTP": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerLoginOTP"
				},
				"dashboard": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerDashboard"
				},
				"products": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerETAList"
				},
				"productDetails": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerETADetails"
				},
				"orderNow": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerOrderNow"
				},
				"orders": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "logList"
				},
				"orderStatus": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "LogsheetDetails"
				},
				"vessel": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "vessel"
				},
				"dashboardHarbour": {
					"viewType": "XML",
					"viewName": "customerDashboard1"
				},
				"dashboardAgent": {
					"viewType": "XML",
					"viewName": "customerDashboardAgent"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zsharjahport/webapp",
		"_version": "1.1.0"
	}
}