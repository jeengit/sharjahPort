{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.demo.sharjahPort",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZSPA_DEMO_SRV": {
				"uri": "/sap/opu/odata/sap/ZSPA_DEMO_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/ZSPA_DEMO_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.demo.sharjahPort.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.demo.sharjahPort.i18n.i18n"
				}
			},
			"s4Model": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZSPA_DEMO_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.demo.sharjahPort.view.customer",
				"controlAggregation": "detailPages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "dashboard",
				"pattern": "dashboard",
				"target": [
					"home",
					"dashboard"
				]
			}, {
				"name": "etaList",
				"pattern": "items={sPath}&type={type}",
				"target": [
					"home",
					"etaList"
				]
			}, {
				"name": "etaDetails",
				"pattern": "etaDetails/{sPath}/{id}",
				"target": [
					"home",
					"etaDetails"
				]
			}, {
				"name": "logList",
				"pattern": "items={sPath}&type={type}",
				"target": [
					"home",
					"logList"
				]
			}, {
				"name": "logDetails",
				"pattern": "logDetails/{sPath}/{id}",
				"target": [
					"home",
					"logDetails"
				]
			}, {
				"name": "vessel",
				"pattern": "vessel",
				"target": [
					"home",
					"vessel"
				]
			}, {
				"name": "dashboardHarbour",
				"pattern": "dashboardHarbour",
				"target": [
					"home",
					"dashboardHarbour"
				]
			}, {
				"name": "dashboardAgent",
				"pattern": "dashboardAgent",
				"target": [
					"home",
					"dashboardAgent"
				]
			}, {
				"name": "dashboardManifest",
				"pattern": "dashboardManifest",
				"target": [
					"home",
					"dashboardManifest"
				]
			}, {
				"name": "manifest",
				"pattern": "items={sPath}&type='{type}'",
				"target": [
					"home",
					"manifest"
				]
			}, {
				"name": "manifestDetails",
				"pattern": "manifestDetails/{sPath}/{id}/{status}",
				"target": [
					"home",
					"manifestDetails"
				]
			}, {
				"name": "deliveryDetails",
				"pattern": "deliveryDetails/{sPath}/{id}",
				"target": [
					"home",
					"deliveryDetails"
				]
			},{
				"name": "tallySheetClerkList",
				"pattern": "tallySheetClerkList",
				"target": [
					"home",
					"tallySheetClerkList"
				]
			},{
				"name": "tallySheetClerkDetails",
				"pattern": "tallySheetClerkDetails/{id}",
				"target": [
					"home",
					"tallySheetClerkDetails"
				]
			},
			{
				"name": "tallySheetCargo",
				"pattern": "tallySheetCargo/{sPath}/{id}",
				"target": [
					"home",
					"tallySheetCargo"
				]
			}],
			"targets": {
				"home": {
					"viewType": "XML",
					"transition": "slide",
					"controlAggregation": "masterPages",
					"viewName": "Home"
				},
				"dashboard": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerDashboardTower"
				},
				"dashboardHarbour": {
					"viewType": "XML",
					"viewName": "customerDashboardHarbour"
				},
				"dashboardAgent": {
					"viewType": "XML",
					"viewName": "customerDashboardAgent"
				},
				"dashboardManifest": {
					"viewType": "XML",
					"viewName": "customerDashboardManifest"
				},
				"etaList": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerETAList"
				},
				"etaDetails": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "customerETADetails"
				},
				"logList": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "logList"
				},
				"logDetails": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "LogsheetDetails"
				},
				"vessel": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "vessel"
				},
				"manifest": {
					"viewType": "XML",
					"viewName": "manifestList"
				},
				"manifestDetails": {
					"viewType": "XML",
					"viewName": "manifestDetails"
				},
				"deliveryDetails": {
					"viewType": "XML",
					"viewName": "deliveryDetails"
				},
				"tallySheetClerkList": {
					"viewType": "XML",
					"viewName": "tallySheetClerkList"
				},
				"tallySheetClerkDetails": {
					"viewType": "XML",
					"viewName": "tallySheetClerkDetails"
				},
				"tallySheetCargo": {
					"viewType": "XML",
					"viewName": "tallySheetCargo"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zsharjahport/webapp",
		"_version": "1.1.0"
	}
}