									<!--<ImageContent src="sap-icon://complete" />-->
								</TileContent>
							</GenericTile>
							<GenericTile class="tileLayout" header="{i18n>PaymentList}" >
								<TileContent unit="" footer="5">
									<!--<ImageContent src="sap-icon://complete" />-->
								</TileContent>
							</GenericTile>
							<GenericTile class="tileLayout" header="Service request" subheader="{i18n>subServ}">
								<TileContent unit="" footer="20">
									<!--<ImageContent src="sap-icon://complete" />-->
								</TileContent>
							</GenericTile>
						</l:content>
					</l:Grid>
