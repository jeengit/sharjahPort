						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>Callsign}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:ComboBox placeholder="{i18n>select}" selectedKey="{etaDetailsModel>/CallSign}" items="{path: 'agentVesselModel>/results'}"
