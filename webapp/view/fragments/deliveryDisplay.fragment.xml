												<m:DatePicker value="{vesMovModel>Shift_FromDate}" valueFormat="yyyyMMdd" displayFormat="long"/>
											</m:HBox>
										</m:HBox>
										
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="{i18n>cargDet}">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:VBox>
								<t:Table rows="{vesMovModel>/LogsheetToShifting/results}" selectionMode="None" id="shifTableChange" ariaLabelledBy="title">
									<t:extension>
										<m:OverflowToolbar style="Clear">
