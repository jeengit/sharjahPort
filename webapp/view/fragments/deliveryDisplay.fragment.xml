									</m:VBox>
								
									</layout:VerticalLayout>
										<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
												<m:VBox>
										<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
											<m:HBox width="50%">
												<m:Label text="{i18n>porthand}"/>
