											<m:HBox width="60%">
												<m:Input value="{vesMovModel>/Anch_PilotRemark}"/>
											</m:HBox>
										</m:HBox>
										<m:HBox  alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>corgin}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:ComboBox selectedKey="{vesMovModel>/Berth_berthNumber}" width="100%" items="{path: 'vesMovBertModel>/'}" placeholder="Select Berth">
													<core:Item key="{vesMovBertModel>BerthNumber}" text="{vesMovBertModel>BerthNumber}"/>
												</m:ComboBox>
											</m:HBox>
										</m:HBox>
										<m:HBox class="" alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>billno}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:Input value="{vesMovModel>/Anch_PilotRemark}"/>
											</m:HBox>
										</m:HBox>
									</m:VBox>
								</layout:VerticalLayout>
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<!--<m:Text class="title sapUiTinyMarginBottom" text="{i18n>consignDet}"/>-->
									<m:VBox>
										<m:HBox  alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>paymod}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:ComboBox selectedKey="{vesMovModel>/Berth_berthNumber}" width="100%" items="{path: 'vesMovBertModel>/'}" placeholder="Select">
													<core:Item key="{vesMovBertModel>BerthNumber}" text="{vesMovBertModel>BerthNumber}"/>
												</m:ComboBox>
											</m:HBox>
										</m:HBox>
										<m:HBox alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>impcode}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:Input value="{vesMovModel>/Anch_PilotRemark}"/>
											</m:HBox>
										</m:HBox>
										<m:HBox  alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>r/no}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:Input value="{vesMovModel>/Anch_PilotRemark}"/>
											</m:HBox>
										</m:HBox>
										<m:HBox  alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>dutyAmn}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:Input value="{vesMovModel>/Anch_PilotRemark}"/>
											</m:HBox>
										</m:HBox>
										<m:HBox  alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>billD}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:DatePicker id="depDateArrival" value="{etaDetailsModel>/ExpArrivalDate}" valueFormat="yyyyMMdd" displayFormat="long"/>
											</m:HBox>
										</m:HBox>
									</m:VBox>
								</layout:VerticalLayout>
								
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<!--<m:Text class="title sapUiTinyMarginBottom" text="{i18n>consignDet}"/>-->
									<m:VBox>
										<m:HBox  alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>tranDet}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:Input value="{vesMovModel>/Anch_PilotRemark}"/>
											</m:HBox>
										</m:HBox>
										<m:HBox alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>Agentcode}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:Input value="{vesMovModel>/Anch_PilotRemark}"/>
											</m:HBox>
										</m:HBox>
										<m:HBox  alignItems="Center">
											<m:HBox width="40%">
												<m:Label text="{i18n>AgentName}"/>
											</m:HBox>
											<m:HBox width="60%">
												<m:Input value="{vesMovModel>/Anch_PilotRemark}"/>
											</m:HBox>
