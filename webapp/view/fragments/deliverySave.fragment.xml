						<!--</m:HBox>-->
						<m:HBox class="sapUiSmallMarginBottom">
							<m:Label text="{i18n>B/Enumber}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{delvryDetailsModel>/BOE}"/>
						</m:HBox>
						<m:HBox class="sapUiSmallMarginBottom">
							<m:Label text="B/L No" class="sapUiSmallMarginEnd"/>
