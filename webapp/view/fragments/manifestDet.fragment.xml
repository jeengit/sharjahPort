<core:FragmentDefinition xmlns:t="sap.ui.table" xmlns="sap.uxap"
	xmlns:m="sap.m" xmlns:layout="sap.ui.layout" xmlns:core="sap.ui.core">
	<ObjectPageLayout upperCaseAnchorBar="false" id="manifestDisp" class="objLayoutClass">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<!--<breadcrumbs>-->
				<!--	<m:Breadcrumbs currentLocationText="{etaDetailsModel>/ZETANumber}">-->
				<!--		<m:Link id="dashboard" press="onHomePress" text="Home"/>-->
				<!--		<m:Link id="products" press="onNavToPress" text="ETA List"/>-->
				<!--	</m:Breadcrumbs>-->
				<!--</breadcrumbs>-->
				<expandedHeading>
					<m:HBox>
						<m:Title text="{manDetModel>/ManifestNo}" wrapping="true"/>
						<m:ObjectMarker type="Flagged" class="sapUiTinyMarginBegin"/>
					</m:HBox>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox fitContainer="true" alignItems="Center">
						<m:Avatar backgroundColor="Accent9" src="sap-icon://manager-insight" class="sapUiTinyMarginEnd"/>
						<m:Title text="{manDetModel>/ManifestNo}" wrapping="true"/>
					</m:FlexBox>
				</snappedHeading>
				<expandedContent>
					<m:Text text="Manifest Details"/>
				</expandedContent>
				<snappedContent>
					<m:Text text="Manifest Details"/>
				</snappedContent>
				<snappedTitleOnMobile>
					<m:Title text="Header Title On Phone"/>
				</snappedTitleOnMobile>
				<!--<actions>-->
				<!--	<m:OverflowToolbarButton icon="sap-icon://edit" text="edit header" type="Emphasized" tooltip="edit"/>-->
				<!--	<m:OverflowToolbarButton icon="sap-icon://pull-down" text="show section" type="Emphasized" tooltip="pull-down"/>-->
				<!--	<m:OverflowToolbarButton icon="sap-icon://show" text="show state" tooltip="show"/>-->
				<!--	<m:Button text="Toggle Footer" press="toggleFooter"/>-->
				<!--</actions>-->
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
			<m:FlexBox wrap="Wrap">
				<m:Avatar class="sapUiSmallMarginEnd" backgroundColor="Accent9" src="sap-icon://manager-insight" displaySize="L"/>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>PortName}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{manDetModel>/PortCode}"/>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>manNo}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{manDetModel>/ManifestNo}"/>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>Captain}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{manDetModel>/Caption}"/>
						</m:HBox>
							<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>disComDate}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{manDetModel>/DischargeCommencedDate}"/>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>ArrivalDate}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{ path: 'manDetModel>/ArrivalDate', type: 'sap.ui.model.type.Date', formatOptions: {source: {pattern: 'dd/MM/yyyy'}, style: 'long'} }"/>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>crtDate}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{ path: 'manDetModel>/CreationDate', type: 'sap.ui.model.type.Date', formatOptions: {source: {pattern: 'dd/MM/yyyy'}, style: 'long'} }"
								wrapping="false"/>
						</m:HBox>
							<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>Agentcode}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{manDetModel>/AgentCode}"
								wrapping="false"/>
						</m:HBox>
							<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>voyNo}" class="sapUiSmallMarginEnd"/>
						<m:Text text="{manDetModel>/VoyegeNo}"/>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>cusManfNo}" class="sapUiSmallMarginEnd"/>
							<m:Text
								text="{manDetModel>/CustRefManifestNo}"
								wrapping="false"/>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>AgentName}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{manDetModel>/AgentName}"/>
						</m:HBox>
							<m:HBox class="sapUiTinyMarginBottom">
							<m:Label text="{i18n>VesselName}" class="sapUiSmallMarginEnd"/>
							<m:Text text="{manDetModel>/VesselName}"/>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
			</m:FlexBox>
		</headerContent>
		<sections>
			<ObjectPageSection titleUppercase="false" title="{i18n>billEntry}">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<t:Table rows="{manDetModel>/BillOfEntrySet/results}" selectionMode="None"
									id="shifTableDisplay" ariaLabelledBy="title">
									<!--<t:extension>-->
									<!--	<OverflowToolbar style="Clear">-->
									<!--		<Title text="{i18n>ShiftingTable}"/>-->
									<!--		<ToolbarSpacer/>-->
									<!--		<Button type="Emphasized" icon="sap-icon://delete" tooltip="Remove" press="addNewRowPress"/>-->
									<!--		<Button press="addNewRowPress" type="Accept" icon="sap-icon://add" tooltip="Add"/>-->
									<!--		<Button type="Emphasized" icon="sap-icon://save" tooltip="Save" press="clearAllSortings" visible="false"/>-->
									<!--	</OverflowToolbar>-->
									<!--</t:extension>-->
									<t:columns>
										<t:Column sortProperty="ETANo">
											<m:Label wrapping="false" text="{i18n>B/ENo}"/>
											<t:template>
													<m:Link text="{manDetModel>BENo}" press="handlePress"/>
											</t:template>
										</t:Column>
										<t:Column  sortProperty="PortName">
											<m:Label text="{i18n>B/LNo}"/>
											<t:template>
													<m:Text text="{manDetModel>BilNo}"/>
											</t:template>
										</t:Column>
										<t:Column>
											<m:Label text="{i18n>consNo}"/>
											<t:template>
												<m:Text text="{manDetModel>ConsigneeCode}" wrapping="false"/>
											</t:template>
										</t:Column>
										<t:Column>
											<m:Label text="{i18n>receiptno}"/>
											<t:template>
												<m:Text text="{manDetModel>RecieptNo}"/>
											</t:template>
										</t:Column>
										<t:Column>
											<m:Label text="{i18n>Cargocategory}"/>
											<t:template>
												<m:Text text="{manDetModel>CargoCategory}"/>
											</t:template>
										</t:Column>
									
										<t:Column>
											<m:Label text="{i18n>Status}"/>
											<t:template>
												<m:Text text="{manDetModel>Status}"/>
											</t:template>
										</t:Column>
									</t:columns>
								</t:Table>
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</core:FragmentDefinition>