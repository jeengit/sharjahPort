							<m:HBox width="40%">
								<m:Label text="{i18n>tallyCode}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input  value="{tSCrtModel>/TallySheetCode}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>PortName}"/>
