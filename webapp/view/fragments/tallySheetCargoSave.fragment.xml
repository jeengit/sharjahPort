					</m:Breadcrumbs>
				</breadcrumbs>
				<expandedHeading>
					<m:HBox>
						<m:Title text="Create TallySheet" wrapping="true"/>
					</m:HBox>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox fitContainer="true" alignItems="Center">
						<m:Avatar backgroundColor="Accent9" src="sap-icon://excel-attachment" class="sapUiTinyMarginEnd"/>
						<m:Title text="Create TallySheet" wrapping="true"/>
					</m:FlexBox>
				</snappedHeading>
				<expandedContent>
					<m:Text text="Tally Sheet"/>
				</expandedContent>
				<snappedContent>
					<m:Text text="Tally Sheet"/>
				</snappedContent>
				<snappedTitleOnMobile>
					<m:Title text="Header Title On Phone"/>
				</snappedTitleOnMobile>
