<core:FragmentDefinition xmlns:t="sap.ui.table" xmlns="sap.uxap" xmlns:m="sap.m" xmlns:layout="sap.ui.layout" xmlns:core="sap.ui.core">
	<ObjectPageLayout upperCaseAnchorBar="false" id="tallyClerkchangId" class="objLayoutClass editMode">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<breadcrumbs>
					<m:Breadcrumbs currentLocationText="{tSClerkDetailModel>/ManifestNo}">
						<m:Link href="#tallySheetClerkList" text="TallySheet"/>
					</m:Breadcrumbs>
				</breadcrumbs>
				<expandedHeading>
					<m:HBox>
						<m:Title text="{tSClerkDetailModel>/ManifestNo} - {tSClerkDetailModel>/VesselName}" wrapping="true"/>
					</m:HBox>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox fitContainer="true" alignItems="Center">
						<m:Avatar backgroundColor="Accent9" src="sap-icon://shipping-status" class="sapUiTinyMarginEnd"/>
						<m:Title text="{tSClerkDetailModel>/ManifestNo} - {tSClerkDetailModel>/VesselName}" wrapping="true"/>
					</m:FlexBox>
				</snappedHeading>
				<expandedContent>
					<m:Text text="Vessel Details"/>
				</expandedContent>
				<snappedContent>
					<m:Text text="Vessel Details"/>
				</snappedContent>
				<snappedTitleOnMobile>
					<m:Title text="Header Title On Phone"/>
				</snappedTitleOnMobile>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
			<m:FlexBox wrap="Wrap">
				<m:Avatar class="sapUiSmallMarginEnd" backgroundColor="Accent9" src="sap-icon://shipping-status" displaySize="L"/>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>VesselName}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/VesselName}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Agent Code"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/AgentCode}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Shift"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/Shift}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Hatch No"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/HatchNo}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Time Commenced"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/TimeCommenced}"/>
							</m:HBox>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>CustomsRefNo}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/CustomsRefNo}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>Callsign}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/Callsign}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/AgentName} === '' ? false: true}" class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>AgentName}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/AgentName}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/AgentCode} === '' ? false: true}" class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>AgentCode}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/AgentCode}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Date"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:DatePicker value="{tSClerkDetailModel>/Date}" valueFormat="yyyyMMdd" displayFormat="long"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/Derrik_Crain} === '' ? false: true}" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Derrik/Crain"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/Derrik_Crain}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/TimeCompleted} === '' ? false: true}" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Time Completed"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/TimeCompleted}"/>
							</m:HBox>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox visible="{= ${tSClerkDetailModel>/TallyServiceNo} === '' ? false: true}" class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Tally(Service) number"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/TallyServiceNo}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>IMOnumber}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/IMO}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/EmployeeId} === '' ? false: true}" class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Employee Id"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/EmployeeId}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Berth"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/Berth}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="No of Hooks"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/NoOfHooks}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="GUID"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/GUID}"/>
							</m:HBox>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
			</m:FlexBox>
		</headerContent>
		<sections>
			<ObjectPageSection titleUppercase="false" title="Cargo Details">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<m:FlexBox justifyContent="End" width="100%">
										<core:Icon color="#333" src="sap-icon://add" alt="X" tooltip="{i18n>sort}" press="addNewRowPress"/>
										</m:FlexBox>
										<t:Table visibleRowCount="5" rows="{tSClerkDetailModel>/CargoDetailsSet/results}" selectionMode="None" ariaLabelledBy="title"
											class="card tableCard no-margin">
											<t:columns>
												<t:Column sortProperty="LogNumber">
													<m:Label text="Sling No"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Sling_No}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="PortCode" width="10rem">
													<m:Label text="Description Package"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Desc_Package}"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="Marks_No" width="10rem">
													<m:Label text="Marks No"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Marks_No}"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="Marks_No">
													<m:Label text="Particulars"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Particular_TS}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Total"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Total}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Gross Weight"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Gross_Weight}"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="Sent_to">
													<m:Label text="Sent To"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Sent_to}"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="Remark">
													<m:Label text="Remark"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Remark}"/>
													</t:template>
												</t:Column>
											</t:columns>
										</t:Table>
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Stevedoring Charge Details">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<m:FlexBox justifyContent="End" width="100%">
										<core:Icon color="#333" src="sap-icon://add" alt="A" tooltip="{i18n>sort}" press="addNewRowPress"/>
										</m:FlexBox>
										<t:Table visibleRowCount="5" rows="{ path: 'tSClerkDetailModel>/ServiceChargeSet/results',filters: {path: 'ItemType', operator: 'EQ', value1: 'A'} }" selectionMode="None" ariaLabelledBy="title"
											class="card tableCard no-margin">
											<t:columns>
												<t:Column sortProperty="Commodity">
													<m:Label text="Commodity"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Commodities}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="PortCode" width="10rem">
													<m:Label text="No of packs"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>NoOfPackage}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Quantity"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Quantity}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="UOM"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>UOM}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="StRate"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Rate}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Amount"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Amount}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Remark"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Remark}"/>
													</t:template>
												</t:Column>
											</t:columns>
										</t:Table>
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Forklift Charge Details">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<m:FlexBox justifyContent="End" width="100%">
										<core:Icon color="#333" src="sap-icon://add" alt="D" tooltip="{i18n>sort}" press="addNewRowPress"/>
										</m:FlexBox>
										<t:Table visibleRowCount="5" rows="{ path: 'tSClerkDetailModel>/ServiceChargeSet/results',filters: {path: 'ItemType', operator: 'EQ', value1: 'D'} }" selectionMode="None" ariaLabelledBy="title"
											class="card tableCard no-margin">
											<t:columns>
												<t:Column sortProperty="FlEquipment">
													<m:Label text="Equipment"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>EquipmentCode}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Hatch no"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>HatchNo}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="FlDate"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Date}"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="FlRate">
													<m:Label text="Rate"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Rate}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Amount"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Amount}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Remark"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Remark}"/>
													</t:template>
												</t:Column>
											</t:columns>
										</t:Table>
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Crane Charge Details">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<m:FlexBox justifyContent="End" width="100%">
										<core:Icon color="#333" src="sap-icon://add" alt="E" tooltip="{i18n>sort}" press="addNewRowPress"/>
										</m:FlexBox>
										<t:Table visibleRowCount="5" rows="{ path: 'tSClerkDetailModel>/ServiceChargeSet/results',filters: {path: 'ItemType', operator: 'EQ', value1: 'E'} }" selectionMode="None" ariaLabelledBy="title"
											class="card tableCard no-margin">
											<t:columns>
												<t:Column sortProperty="CrEquipment">
													<m:Label text="Equipment"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Equipment}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Hatch no"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>HatchNo}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Date"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Date}"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="CrWeight">
													<m:Label text="Weight"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Weight}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="CrRate">
													<m:Label text="Rate"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Rate}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Amount"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Amount}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Remark"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Remark}"/>
													</t:template>
												</t:Column>
											</t:columns>
										</t:Table>
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Supplementary Charge Details">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<m:FlexBox justifyContent="End" width="100%">
										<core:Icon color="#333" src="sap-icon://add" alt="F" tooltip="{i18n>sort}" press="addNewRowPress"/>
										</m:FlexBox>
										<t:Table visibleRowCount="5" rows="{ path: 'tSClerkDetailModel>/ServiceChargeSet/results',filters: {path: 'ItemType', operator: 'EQ', value1: 'F'} }" selectionMode="None" ariaLabelledBy="title"
											class="card tableCard no-margin">
											<t:columns>
												<t:Column>
													<m:Label text="Date"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Date}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="From"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>FromTime}"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="FromTo">
													<m:Label text="To"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>FromTo}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Rate"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Rate}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Amount"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Amount}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Remark"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Remark}"/>
													</t:template>
												</t:Column>
											</t:columns>
										</t:Table>
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Miscellaneous Charge Details">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<m:FlexBox justifyContent="End" width="100%">
										<core:Icon color="#333" src="sap-icon://add" alt="H" tooltip="{i18n>sort}" press="addNewRowPress"/>
										</m:FlexBox>
										<t:Table visibleRowCount="5" rows="{ path: 'tSClerkDetailModel>/ServiceChargeSet/results',filters: {path: 'ItemType', operator: 'EQ', value1: 'H'} }" selectionMode="None" ariaLabelledBy="title"
											class="card tableCard no-margin">
											<t:columns>
												<t:Column>
													<m:Label text="Date"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Date}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Rate"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Rate}"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="MsCharge">
													<m:Label text="Charge"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Charge}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="MsRemarks"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Remarks}"/>
													</t:template>
												</t:Column>
											</t:columns>
										</t:Table>
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
