<core:FragmentDefinition xmlns:t="sap.ui.table" xmlns="sap.uxap" xmlns:m="sap.m" xmlns:layout="sap.ui.layout" xmlns:core="sap.ui.core">
	<ObjectPageLayout upperCaseAnchorBar="false" id="tallyClerkchangId" class="objLayoutClass editMode">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<breadcrumbs>
					<m:Breadcrumbs currentLocationText="{tSClerkDetailModel>/LogSheetNo}">
						<m:Link press="onHomePress" text="Manifest"/>
						<m:Link press="onNavToPress" text="Tally Sheet"/>
					</m:Breadcrumbs>
				</breadcrumbs>
				<expandedHeading>
					<m:HBox>
						<m:Title text="{tSClerkDetailModel>/ManifestNo} - {tSClerkDetailModel>/VesselName}" wrapping="true"/>
						<m:ObjectMarker type="Flagged" class="sapUiTinyMarginBegin"/>
					</m:HBox>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox fitContainer="true" alignItems="Center">
						<m:Avatar backgroundColor="Accent9" src="sap-icon://shipping-status" class="sapUiTinyMarginEnd"/>
						<m:Title text="{tSClerkDetailModel>/ManifestNo} - {tSClerkDetailModel>/VesselName}" wrapping="true"/>
					</m:FlexBox>
				</snappedHeading>
				<expandedContent>
					<m:Text text="Vessel Details"/>
				</expandedContent>
				<snappedContent>
					<m:Text text="Vessel Details"/>
				</snappedContent>
				<snappedTitleOnMobile>
					<m:Title text="Header Title On Phone"/>
				</snappedTitleOnMobile>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
			<m:FlexBox wrap="Wrap">
				<m:Avatar class="sapUiSmallMarginEnd" backgroundColor="Accent9" src="sap-icon://shipping-status" displaySize="L"/>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>VesselName}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input id="vnameInp" value="{tSClerkDetailModel>/VesselName}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Agent Code"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" id="portInp" value="{tSClerkDetailModel>/AgentCode}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Shift"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input id="vrnInp" value="{tSClerkDetailModel>/Shift}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Hatch No"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input id="agentInp" value="{tSClerkDetailModel>/HatchNo}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Time Commenced"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/TimeCommenced}"/>
							</m:HBox>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>CustomsRefNo}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/CustomsRefNo}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>Callsign}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/Callsign}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/AgentName} === '' ? false: true}" class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>AgentName}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/AgentName}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/AgentCode} === '' ? false: true}" class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>AgentCode}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/AgentCode}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Date"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:DatePicker value="{tSClerkDetailModel>/Date}" valueFormat="yyyyMMdd" displayFormat="long"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/Derrik_Crain} === '' ? false: true}" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Derrik/Crain"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/Derrik_Crain}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/TimeCompleted} === '' ? false: true}" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Time Completed"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/TimeCompleted}"/>
							</m:HBox>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
						<m:HBox visible="{= ${tSClerkDetailModel>/TallyServiceNo} === '' ? false: true}" class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Tally(Service) number"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/TallyServiceNo}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="{i18n>IMOnumber}"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/IMO}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox visible="{= ${tSClerkDetailModel>/EmployeeId} === '' ? false: true}" class="sapUiTinyMarginBottom" alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Employee Id"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/EmployeeId}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="Berth"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/Berth}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="No of Hooks"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input value="{tSClerkDetailModel>/NoOfHooks}"/>
							</m:HBox>
						</m:HBox>
						<m:HBox alignItems="Center">
							<m:HBox width="40%">
								<m:Label text="GUID"/>
							</m:HBox>
							<m:HBox width="60%">
								<m:Input enabled="false" value="{tSClerkDetailModel>/GUID}"/>
							</m:HBox>
						</m:HBox>
					</m:VBox>
				</layout:VerticalLayout>
			</m:FlexBox>
		</headerContent>
		<sections>
			<ObjectPageSection titleUppercase="false" title="Charge Details">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<t:Table visibleRowCount="5" rows="{tSClerkDetailModel>/ChargeDetailsSet/results}" selectionMode="None" ariaLabelledBy="title" class="card tableCard no-margin">
											<t:extension>
												<m:OverflowToolbar style="Clear">
													<m:Title text="Charge Table ({tSClerkDetailModel>/ChargeDetailsSet/results/length})"/>
													<m:ToolbarSpacer/>
													<core:Icon src="sap-icon://add" tooltip="{i18n>sort}" press="addNewRowPress"/>
												</m:OverflowToolbar>
											</t:extension>
											<t:columns>
												<t:Column sortProperty="LogNumber">
													<m:Label text="Equipment"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>CrEquipment}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="PortCode" width="10rem">
													<m:Label text="Date"/>
													<t:template>
															<m:Text
																text="{ path: 'tSClerkDetailModel>CrDate', type: 'sap.ui.model.type.Date', formatOptions: {source: {pattern: 'dd/MM/yyyy'}, style: 'long'} }"
																wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Weight"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>CrWeight}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Amount"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>CrAmount}"/>
													</t:template>
												</t:Column>
											</t:columns>
										</t:Table>
									</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Cargo Details">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:FlexBox wrap="Wrap">
								<layout:VerticalLayout class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
									<m:VBox>
										<t:Table visibleRowCount="5" rows="{tSClerkDetailModel>/CargoDetailsSet/results}" selectionMode="None" ariaLabelledBy="title" class="card tableCard no-margin">
											<t:extension>
												<m:OverflowToolbar style="Clear">
													<m:Title text="Cargo Table ({tSClerkDetailModel>/CargoDetailsSet/results/length})"/>
													<m:ToolbarSpacer/>
													<core:Icon src="sap-icon://add" tooltip="{i18n>sort}" press="addNewRowPress"/>
												</m:OverflowToolbar>
											</t:extension>
											<t:columns>
												<t:Column sortProperty="LogNumber">
													<m:Label text="Guid"/>
													<t:template>
														<m:Link press="handleDetailsPress" text="{tSClerkDetailModel>GUID}" wrapping="false"/>
													</t:template>
												</t:Column>
												<t:Column sortProperty="PortCode" width="10rem">
													<m:Label text="Package"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Desc_Package}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Total"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Total}"/>
													</t:template>
												</t:Column>
												<t:Column>
													<m:Label text="Gross Weight"/>
													<t:template>
														<m:Text text="{tSClerkDetailModel>Gross_Weight}"/>
													</t:template>
												</t:Column>
											</t:columns>
										</t:Table>
										</m:VBox>
								</layout:VerticalLayout>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</core:FragmentDefinition>