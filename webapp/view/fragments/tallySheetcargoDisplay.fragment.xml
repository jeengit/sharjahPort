				<breadcrumbs>
					<m:Breadcrumbs currentLocationText="Tally Sheet">
						<m:Link press="onHomePress" text="Manifest"/>
					</m:Breadcrumbs>
				</breadcrumbs>
				<expandedHeading>
					<m:HBox>
						<m:Title text="{tSDetailModel>/TallySheetCode}" wrapping="true"/>
					</m:HBox>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox fitContainer="true" alignItems="Center">
