					</m:FlexBox>
				</snappedHeading>
				<expandedContent>
					<m:Text text="Tally Sheet"/>
				</expandedContent>
				<snappedContent>
					<m:Text text="Tally Sheet"/>
				</snappedContent>
				<snappedTitleOnMobile>
